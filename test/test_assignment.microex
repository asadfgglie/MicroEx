program test_assignment

begin
    declare a as integer;
    declare b, c, d[1][3] as real;

    a := 65;
    b := (a - 1) * (a + 1);
    c := a * a - 1;
    d[0][0] := a;
    d[0][1] := b;
    d[0][2] := c;

    declare e as real;

    e := a / b * (d[0][0] + d[0][1]);
end